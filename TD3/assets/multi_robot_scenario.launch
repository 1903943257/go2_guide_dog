<!-- 这里只启动gazebo环境 -->

<launch>
<arg name="gui" value="false"/>

  <!-- Robot with diff drive -->
  <include file="$(find multi_robot_scenario)/launch/go2_gazebo.launch">
     <!-- <arg name="gui" value="$(arg gui)"/> -->
     <arg name="robot_name" value="go2" />
     <arg name="robot_position" value="-x 0.0 -y 0.0 -z 0.01 -R 0 -P 0 -Y +0.0" />
   </include>

  <!-- RVIZ -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find multi_robot_scenario)/launch/go2.rviz" />
</launch> 
